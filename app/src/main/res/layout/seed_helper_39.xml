<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <Button
        android:id="@+id/버튼타이틀"
        android:layout_width="60dp"
        android:layout_height="30dp"
        android:background="@drawable/roundbtnwhite"
        android:fontFamily="@font/jalnan"
        android:gravity="center"
        android:textColor="@color/teal_700"
        android:text="돌아가기"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/시드23"
        />
    <Button
        android:id="@+id/시드23"
        android:layout_width="60dp"
        android:layout_height="94dp"
        android:background="@drawable/roundbtnviolet"
        android:fontFamily="@font/jalnan"
        android:text="23"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@id/버튼타이틀"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/시드24"
        />
    <Button
        android:id="@+id/시드24"
        android:layout_width="60dp"
        android:layout_height="94dp"
        android:background="@drawable/roundbtnviolet"
        android:fontFamily="@font/jalnan"
        android:text="24"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@id/시드23"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/시드36"
        />
    <Button
        android:id="@+id/시드36"
        android:layout_width="60dp"
        android:layout_height="94dp"
        android:background="@drawable/roundbtnviolet"
        android:fontFamily="@font/jalnan"
        android:text="36"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@id/시드24"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/시드39"
        />
    <Button
        android:id="@+id/시드39"
        android:layout_width="60dp"
        android:layout_height="94dp"
        android:background="@drawable/roundbutton"
        android:fontFamily="@font/jalnan"
        android:text="39"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@id/시드36"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/시드47"
        />
    <Button
        android:id="@+id/시드47"
        android:layout_width="60dp"
        android:layout_height="94dp"
        android:background="@drawable/roundbtnviolet"
        android:fontFamily="@font/jalnan"
        android:text="47"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@id/시드39"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/시드48"
        />
    <Button
        android:id="@+id/시드48"
        android:layout_width="60dp"
        android:layout_height="94dp"
        android:background="@drawable/roundbtnviolet"
        android:fontFamily="@font/jalnan"
        android:text="48"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@id/시드47"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/시드49"
        />
    <Button
        android:id="@+id/시드49"
        android:layout_width="60dp"
        android:layout_height="94dp"
        android:background="@drawable/roundbtnviolet"
        android:fontFamily="@font/jalnan"
        android:text="49"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@id/시드48"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <TextView
        android:id="@+id/타이틀"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/roundbutton"
        android:fontFamily="@font/jalnan"
        android:gravity="center"
        android:text="시드 39층"
        android:textSize="29dp"
        app:layout_constraintLeft_toRightOf="@id/버튼타이틀"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <androidx.appcompat.widget.SearchView
        android:id="@+id/검색"
        android:layout_margin="2dp"
        android:layout_marginBottom="2dp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/border"
        app:queryHint="문제 또는 답 검색(예:골드비치)"
        app:iconifiedByDefault="false"
        app:layout_constraintLeft_toRightOf="@id/시드23"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/타이틀"
        />

    <TextView
        android:id="@+id/골드비치질문"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="골드비치 리조트에 거주하는 잡화 상인 이름은?"
        android:layout_marginLeft="4dp"
        android:textColor="@color/black"
        android:layout_marginBottom="4dp"
        app:layout_constraintTop_toBottomOf="@id/검색"
        app:layout_constraintLeft_toRightOf="@id/시드23"

        />
    <Button
        android:id="@+id/골드비치1번"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@drawable/roundbutton"
        android:text="1.스크루지"
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@id/골드비치질문"
        app:layout_constraintLeft_toRightOf="@id/시드23"
        app:layout_constraintRight_toLeftOf="@id/골드비치2번"

        android:layout_marginLeft="2dp"
        />
    <Button
        android:id="@+id/골드비치2번"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@drawable/roundbutton"
        android:text="2.나탈리"
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@id/골드비치질문"
        app:layout_constraintLeft_toRightOf="@id/골드비치1번"
        app:layout_constraintRight_toLeftOf="@id/골드비치3번"

        android:layout_marginLeft="2dp"

        />
    <Button
        android:id="@+id/골드비치3번"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@drawable/roundbtnviolet"
        android:text="3.메이슨"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@id/골드비치질문"
        app:layout_constraintLeft_toRightOf="@id/골드비치2번"
        app:layout_constraintRight_toLeftOf="@id/골드비치4번"

        android:layout_marginLeft="2dp"

        />
    <Button
        android:id="@+id/골드비치4번"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@drawable/roundbutton"
        android:text="4.베티"
        android:textSize="15dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@id/골드비치3번"
        app:layout_constraintTop_toBottomOf="@id/골드비치질문"

        android:layout_marginLeft="2dp"

        />
    <com.google.android.material.divider.MaterialDivider
        android:id="@+id/경계선"
        android:layout_width="0dp"
        android:layout_height="5dp"
        app:layout_constraintLeft_toRightOf="@id/시드23"
        app:layout_constraintTop_toBottomOf="@id/골드비치1번"
        app:layout_constraintRight_toRightOf="parent"
        />
</androidx.constraintlayout.widget.ConstraintLayout>


