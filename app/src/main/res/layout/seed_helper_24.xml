<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <Button
        android:id="@+id/버튼타이틀"
        android:layout_width="60dp"
        android:layout_height="0dp"
        android:background="@drawable/roundbtnwhite"
        android:fontFamily="@font/jalnan"
        android:gravity="center"
        android:textColor="@color/teal_700"
        android:text="시드 층"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/시드23"
        app:layout_constraintVertical_weight="1"
        android:layout_margin="2dp"
        />
    <Button
        android:id="@+id/시드23"
        android:layout_width="60dp"
        android:layout_height="0dp"
        android:background="@drawable/roundbtnviolet"
        android:fontFamily="@font/jalnan"
        android:text="23"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@id/버튼타이틀"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/시드24"
        app:layout_constraintVertical_weight="2"
        android:layout_margin="2dp"
        />
    <Button
        android:id="@+id/시드24"
        android:layout_width="60dp"
        android:layout_height="0dp"
        android:background="@drawable/roundbutton"
        android:fontFamily="@font/jalnan"
        android:text="24"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@id/시드23"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/시드36"
        app:layout_constraintVertical_weight="2"
        android:layout_margin="2dp"
        />
    <Button
        android:id="@+id/시드36"
        android:layout_width="60dp"
        android:layout_height="0dp"
        android:background="@drawable/roundbtnviolet"
        android:fontFamily="@font/jalnan"
        android:text="36"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@id/시드24"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/시드39"
        app:layout_constraintVertical_weight="2"
        android:layout_margin="2dp"
        />
    <Button
        android:id="@+id/시드39"
        android:layout_width="60dp"
        android:layout_height="0dp"
        android:background="@drawable/roundbtnviolet"
        android:fontFamily="@font/jalnan"
        android:text="39"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@id/시드36"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/시드47"
        app:layout_constraintVertical_weight="2"
        android:layout_margin="2dp"
        />
    <Button
        android:id="@+id/시드47"
        android:layout_width="60dp"
        android:layout_height="0dp"
        android:background="@drawable/roundbtnviolet"
        android:fontFamily="@font/jalnan"
        android:text="47"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@id/시드39"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/시드48"
        app:layout_constraintVertical_weight="2"
        android:layout_margin="2dp"
        />
    <Button
        android:id="@+id/시드48"
        android:layout_width="60dp"
        android:layout_height="0dp"
        android:background="@drawable/roundbtnviolet"
        android:fontFamily="@font/jalnan"
        android:text="48"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@id/시드47"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/시드49"
        app:layout_constraintVertical_weight="2"
        android:layout_margin="2dp"
        />
    <Button
        android:id="@+id/시드49"
        android:layout_width="60dp"
        android:layout_height="0dp"
        android:background="@drawable/roundbtnviolet"
        android:fontFamily="@font/jalnan"
        android:text="49"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@id/시드48"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_weight="2"
        android:layout_margin="2dp"
        />
    <TextView
        android:id="@+id/타이틀"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/jalnan"
        android:textSize="29dp"
        android:text="시드 24층"
        android:gravity="center"
        android:background="@drawable/roundbutton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@id/버튼타이틀"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@id/검색"
        />
    <androidx.appcompat.widget.SearchView
        android:id="@+id/검색"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/타이틀"
        android:layout_margin="2dp"
        android:background="@drawable/border"
        app:iconifiedByDefault="false"
        app:queryHint="힌트 검색(예: 마을이름,설명)"
        app:layout_constraintLeft_toRightOf="@id/시드23"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/타이틀"
        app:layout_constraintBottom_toTopOf="@id/리스항구"
        />
    <View
        android:id="@+id/리스항구"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:background="@drawable/border"
        app:layout_constraintTop_toBottomOf="@id/검색"
        app:layout_constraintLeft_toRightOf="@id/시드23"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@id/리스항구이미지"
        />
    <ImageView
        android:id="@+id/리스항구이미지"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:background="@drawable/less"
        app:layout_constraintTop_toTopOf="@id/리스항구"
        app:layout_constraintLeft_toRightOf="@id/시드23"
        app:layout_constraintRight_toLeftOf="@id/리스항구텍스트"
        app:layout_constraintBottom_toBottomOf="@id/리스항구"
        />
    <TextView
        android:id="@+id/리스항구텍스트"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:fontFamily="@font/jalnan"
        android:textSize="10dp"
        android:text="리스항구"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="@id/리스항구"
        app:layout_constraintLeft_toRightOf="@id/리스항구이미지"
        app:layout_constraintRight_toLeftOf="@id/리스항구설명"
        app:layout_constraintBottom_toBottomOf="@id/리스항구"
        />
    <TextView
        android:id="@+id/리스항구설명"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="푸른빛 바다가 보이는 부둣가에서 갈매기가 날아다니는 것을 바라보고 있는 느낌이 들어."
        app:layout_constraintTop_toTopOf="@id/리스항구"
        app:layout_constraintLeft_toRightOf="@id/리스항구텍스트"
        app:layout_constraintRight_toRightOf="@id/리스항구"
        app:layout_constraintBottom_toBottomOf="@id/리스항구"

        />


</androidx.constraintlayout.widget.ConstraintLayout>